{
  "name": "component",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "make run-android",
    "ios": "make run-ios",
    "cleanBundleAndroid": "rm android/app/src/main/assets/index.android.bundle",
    "createBundleAndroid": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "createBundleIos": "react-native bundle --minify --entry-file index.js --platform ios --dev false --bundle-output ./ios/main.jsbundle --assets-dest ./ios",
    "convertSvgToJs": "npx @svgr/cli --native  -d assets/svgs assets/images",
    "eslint": "eslint app __tests__ index.* --ext .js .jsx --cache",
    "eslint:fix": "npm run eslint -- --fix",
    "generateAndroid": "npm run cleanBundleAndroid && npm run createBundleAndroid && cd android && ./gradlew clean && ./gradlew assembleRelease",
    "precommit": "npm run eslint",
    "prepush": "npm run eslint",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:updateSnapshot": "npm run test -- --updateSnapshot",
    "test:coverage": "npm run test -- --coverage ",
    "postinstall": "node ./fixAndroid/android-release-fix.js",
    "run:android": "react-native run-android",
    "start:reset": "react-native start --reset-cache",
    "env:dev": "./prepare-env.sh .env.dev .env && ./setup-google-services.sh ./google-service/android/google-services.dev.json ./android/app/google-services.json && ./setup-google-services.sh ./google-service/ios/GoogleService-Info.dev.plist ./ios/GoogleService-Info.plist && npm run start:reset",
    "env:stage": "./prepare-env.sh .env.uat .env && ./setup-google-services.sh ./google-service/android/google-services.stage.json ./android/app/google-services.json && ./setup-google-services.sh ./google-service/ios/GoogleService-Info.stage.plist ./ios/GoogleService-Info.plist && npm run start:reset",
    "env:prod": "./prepare-env.sh .env.prod .env && ./setup-google-services.sh ./google-service/android/google-services.prod.json ./android/app/google-services.json && ./setup-google-services.sh ./google-service/ios/GoogleService-Info.prod.plist ./ios/GoogleService-Info.plist && npm run start:reset"
  },
  "dependencies": {
    "lodash": "^4.17.13",
    "moment": "^2.24.0",
    "react": "16.8.3",
    "react-native": "^0.59.2",
    "react-native-i18n": "^2.0.14",
    "react-native-masonry": "^0.5.0-alpha.5",
    "react-native-snap-carousel": "^3.8.3",
    "react-native-svg": "^9.13.3",
    "react-navigation": "^1.5.8",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "^0.10.15"
  },
  "devDependencies": {
    "@svgr/cli": "^3.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "husky": "^0.14.3",
    "jest": "23.6.0",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.1",
    "react-dom": "^16.6.1",
    "react-test-renderer": "16.6.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/tests/assetsTransformer.js"
    },
    "setupFiles": [
      "./tests/enzymeTestAdapterSetup.js"
    ],
    "collectCoverageFrom": [
      "app/components/**/*.js",
      "app/screens/**/*.js",
      "app/utils/**/*.js"
    ],
    "transformIgnorePatterns": []
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
